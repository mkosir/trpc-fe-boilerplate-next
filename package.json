{
  "name": "trpc-fe-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "trpc-api-import": "ts-node trpc-api-import/index.ts",
    "prepare": "husky install",
    "dev": "open http://localhost:3000 && NEXT_PUBLIC_BASE_URL_TRPC_API=http://localhost:4000/trpc next dev",
    "build": "next build",
    "lint": "next lint",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l ."
  },
  "dependencies": {
    "@trpc/client": "10.29.1",
    "@trpc/react-query": "10.29.1",
    "@trpc/server": "10.29.1",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.2.6",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "octokit": "2.0.19",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  }
}
